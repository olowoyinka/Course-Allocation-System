@model MappingLectureCourse.Models.ContentViewModel.Session

@{
    ViewData["Title"] = "Detail";
}

<h3>Detail of the Session</h3>

<br />

<h3 class="text-danger">@ViewData["Exist"]</h3>

<hr />

<div class="row">
    <div class="col-md-4 col-4"><a asp-action="Index" class="btn btn-primary">List of Session</a></div>
    <div class="offset-4 col-md-4 col-4"><a asp-action="Update" asp-route-id="@Model.SessionID" class="btn btn-primary">Update the Session</a></div>
</div>

<hr />


<div>
    <h4>Session</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.SessionName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SessionName)
        </dd>
    </dl>
</div>

